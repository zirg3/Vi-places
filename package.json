    "next": "12.2.5",
    "next-auth-sanity": "^1.4.4",
    "next-sanity": "^0.8.2",
    "nextjs-progressbar": "0.0.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.34.2",
    "react-icons": "^4.4.0",
    "react-loading-skeleton": "^3.1.0",
